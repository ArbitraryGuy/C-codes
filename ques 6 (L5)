#include <iostream>
using namespace std;

int main() {

    int marks;
        cout << "Enter your marks" << endl;
        cin >> marks;

        if (marks >= 90) {
            cout << "Congratulations, you have A+ grade" << endl;

        } 

        else if (marks >=80) {
            cout << "Congratulations, you have A grade" << endl;
        }

        else if (marks >=70) {
            cout << "You have B+ grade" << endl;
        }

        else if (marks >=60) {
            cout << "You have B grade" << endl;
        }

        else if (marks >=50){
            cout << "You have C grade" << endl;
        }

        else if (marks >=40){
            cout << "You have D grade" << endl;
        } 

        else if (marks >=30) {
            cout << "You have E grade" << endl;
        }

        else if (marks <=30) {
            cout << "You have F grade" << endl;
        }

        else {
            cout << "Enter valid marks" << endl;
        }

        return 0;

 }
