#include <iostream>
using namespace std;

int main() {

    int n;
    cout << "Enter your number" << endl;
    cin >> n;

    if(n<0) {

        int a;
        a = (n)*-1;
        cout << "Absolute number is :- " << a <<endl;

    }

    else {
        cout <<"You entered absolutely absolute number :- " << n << endl;
    }

    return 0;
}
