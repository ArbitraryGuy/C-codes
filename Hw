#include <iostream>
using namespace std;

int main() {
    int num1;
        cout<< "Enter first number" << endl;
    cin >> num1;

    int num2;
        cout<< "Enter second number" << endl;
    cin >> num2;

    if (num1==num2) {
        cout<< "Both numbers are equal" << endl;

    }

        else {
            cout <<"Both numbers are different" <<endl;
        }
    
    cout << "Question 1 done" << endl;
    
